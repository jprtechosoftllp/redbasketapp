{
  "name": "@meato/auth-service",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "ts-node --esm src/main.ts"
  },
  "nx": {
    "name": "@meato/auth-service",
    "projectType": "application",
    "sourceRoot": "apps/auth-service/src",
    "targets": {
      "serve": {
        "continuous": true,
        "executor": "@nx/js:node",
        "defaultConfiguration": "development",
        "dependsOn": [
          "build"
        ],
        "options": {
          "buildTarget": "@meato/auth-service:build",
          "runBuildTargetDependencies": false
        },
        "configurations": {
          "development": {
            "buildTarget": "@meato/auth-service:build:development"
          },
          "production": {
            "buildTarget": "@meato/auth-service:build:production"
          }
        }
      },
      "test": {
        "options": {
          "passWithNoTests": true
        }
      }
    }
  },
  "dependencies": {
    "bcryptjs": "3.0.3",
    "cookie-parser": "1.4.7",
    "dotenv": "17.2.3",
    "drizzle-orm": "0.44.7",
    "ejs": "3.1.10",
    "express": "4.21.2",
    "ioredis": "^5.8.2",
    "pg": "^8.16.3",
    "jsonwebtoken": "9.0.2",
    "nodemailer": "7.0.10",
    "tslib": "^2.8.1",
    "axios": "^1.6.0"
  }
}