{
  "name": "@meato/admin-service",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "ts-node --esm src/main.ts"
  },
  "nx": {
    "name": "@meato/admin-service",
    "projectType": "application",
    "sourceRoot": "admin-service/src",
    "targets": {
      "serve": {
        "continuous": true,
        "executor": "@nx/js:node",
        "defaultConfiguration": "development",
        "dependsOn": [
          "build"
        ],
        "options": {
          "buildTarget": "@meato/admin-service:build",
          "runBuildTargetDependencies": false
        },
        "configurations": {
          "development": {
            "buildTarget": "@meato/admin-service:build:development"
          },
          "production": {
            "buildTarget": "@meato/admin-service:build:production"
          }
        }
      },
      "test": {
        "options": {
          "passWithNoTests": true
        }
      }
    }
  },
  "dependencies": {
    "cookie-parser": "1.4.7",
    "cors": "2.8.5",
    "dotenv": "17.2.3",
    "drizzle-orm": "0.44.7",
    "express": "^4.21.2",
    "jsonwebtoken": "9.0.2",
    "morgan": "1.10.1",
    "pg": "^8.16.3",
    "tslib": "^2.3.0"
  }
}