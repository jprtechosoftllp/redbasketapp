{
  "name": "@meato/admin-ui",
  "projectType": "application",
  "sourceRoot": "apps/admin-ui/src",
  "build": {
    "executor": "@nx/next:build",
    "outputs": [
      "{options.outputPath}"
    ],
    "options": {
      "outputPath": "dist/admin-ui",
      "root": "apps/admin-ui",
      "nextConfig": "apps/admin-ui/next.config.js"
    }
  },
  "serve": {
    "executor": "@nx/next:server",
    "options": {
      "buildTarget": "admin-ui:build"
    }
  }
}