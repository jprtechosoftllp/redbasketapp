{
  "name": "@meato/vendor-ui",
  "projectType": "application",
  "sourceRoot": "apps/vendor-ui/src",
  "build": {
    "executor": "@nx/next:build",
    "outputs": [
      "{options.outputPath}"
    ],
    "options": {
      "outputPath": "dist/apps/vendor-ui",
      "root": "apps/vendor-ui",
      "nextConfig": "apps/vendor-ui/next.config.js"
    }
  },
  "serve": {
    "executor": "@nx/next:server",
    "options": {
      "buildTarget": "vendor-ui:build"
    }
  }
}